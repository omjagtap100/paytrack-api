{
  "realm": "paytrack",
  "enabled": true,
  "displayName": "PayTrack Employee Management System",
  "roles": {
    "realm": [
      {
        "name": "isadmin",
        "description": "Administrator role with full system access"
      },
      {
        "name": "isaccountsection", 
        "description": "Accounts section role for financial operations"
      },
      {
        "name": "isHr",
        "description": "Human Resources role for employee management"
      },
      {
        "name": "ismanager",
        "description": "Manager role for team leadership and oversight"
      },
      {
        "name": "employee",
        "description": "Basic employee role"
      }
    ]
  },
  "clients": [
    {
      "clientId": "employees",
      "name": "PayTrack Employee Client",
      "enabled": true,
      "publicClient": false,
      "secret": "employees-client-secret-2024",
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "redirectUris": [
        "http://localhost:3000/*",
        "https://paytrack.company.com/*"
      ],
      "webOrigins": [
        "http://localhost:3000",
        "https://paytrack.company.com"
      ],
      "attributes": {
        "access.token.lifespan": "300",
        "refresh.token.lifespan": "1800"
      },
      "protocolMappers": [
        {
          "name": "roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "config": {
            "multivalued": "true",
            "userinfo.token.claim": "true",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "roles"
          }
        },
        {
          "name": "employee-id",
          "protocol": "openid-connect", 
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "config": {
            "userinfo.token.claim": "true",
            "user.attribute": "employeeId",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "employeeId"
          }
        },
        {
          "name": "department",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper", 
          "config": {
            "userinfo.token.claim": "true",
            "user.attribute": "department",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "department"
          }
        }
      ]
    }
  ],
